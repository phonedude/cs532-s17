ction getDSTstart(){\n                d = new Date(2016, 2, 1, 0, 0, 0, 0);\n                var day = 0;\n                if (d.getDay() != 0) {\n                day = 8-(d.getDay())\n                day = day + 7\n                d = d.setDate(day)\n                DST_start = new Date(d)\n                }\n            }\n            function getDSTend(){\n                d = new Date(2016, 10, 1, 0, 0, 0, 0);\n                var day = 0;\n                if (d.getDay() != 0) {\n                day = 8-(d.getDay())\n                d = d.setDate(day)\n                DST_end = new Date(d)\n                }\n            }\n            function setFooterTime(){\n             getDSTstart();\n             getDSTend();\n            //EST\n            clientDate = new Date();\n            if(clientDate >= DST_start && clientDate <=DST_end){\n                offset = -4;\n            }else{\n                offset = -5;\n            }\n            \n            utc = clientDate.getTime() + (clientDate.getTimezoneOffset() * 60000);\n            serverDate = new Date(utc + (3600000*offset));\n            var dt = serverDate\n            var hours = dt.getHours();\n            //it is pm if hours from 12 onwards\n            suffix = (hours >= 12)? \'p.m.\' : \'a.m.\';\n            //only -12 from hours if it is greater than 12 (if not back at mid night)\n            hours = (hours > 12)? hours -12 : hours;\n            //if 00 then it is 12 am\n            hours = (hours == \'00\')? 12 : hours;\n            var minutes = dt.getMinutes();\n            minutes = (minutes < 10)? \'0\'+minutes : minutes;\n            var time = hours+\':\'+minutes+\' \'+suffix\n            $(\'.realTime\').text(time)\n            $(\'#copy_year\').html(dt.getFullYear());\n            }\n            $(document).ready(function(){\n            \n            setFooterTime();\n            setInterval(function(){ setFooterTime(); }, 10000);\n            $(\'.mobileMenu .main_nav .mega .inner .left\').prepend(\'<a href="javascript:goback();">Back', u'\')\n            $(\'.mobileMenu .main_nav\').click(function(){\n                $(this).children(\'.mega\').css(\'right\',\'0\')\n            })\n            $(\'.hamMenu\').click(function(){\n                if($(this).hasClass(\'active\')){\n                    hideMenu();\n                }else{\n                    showMenu();\n                }\n            });\n            $(\'.menuBack\').bind("click",function(){\n                hideMenu();\n            })\n            });\n        ', u'\n', u'\n', u'\n        .main-slide-img-0{\n        background-position: bottom;\n        background-image:url("https://www.liberty.edu/media/1971/main_slideshow_img/sizes/Since-1971-Training-Champions_1920.jpg");\n        }\n        @media all and (max-width: 1366px) {\n        .main-slide-img-0{\n        background-image:url("https://www.liberty.edu/media/1971/main_slideshow_img/sizes/Since-1971-Training-Champions_1366.jpg");\n        }\n        }\n        @media all and (max-width: 1024px) {\n        .main-slide-img-0{\n        background-image:url("https://www.liberty.edu/media/1971/main_slideshow_img/sizes/Since-1971-Training-Champions_1024.jpg");\n        }\n        }\n        @media all and (max-width: 960px) {\n        .main-slide-img-0{\n        background-image:url("https://www.liberty.edu/media/1971/main_slideshow_img/sizes/Since-1971-Training-Champions_960.jpg");\n        }\n        }\n        @media all and (max-width: 400px) {\n        .main-slide-img-0{\n        background-image:url("https://www.liberty.edu/media/1971/main_slideshow_img/sizes/Since-1971-Training-Champions_400.jpg");\n        }\n        }\n        .main-slide-img-1{\n        background-position: center;\n        background-image:url("https://www.liberty.edu/media/1971/main_slideshow_img/sizes/LUCOM-Outreach_1920.jpg");\n        }\n        @media all and (max-width: 1366px) {\n        .main-slide-img-1{\n        background-image:url("https:/